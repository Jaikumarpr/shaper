{
    "env": {
        "node": true,
        "es2021": true
    },
    "plugins": [
        "prettier"
    ],
    "extends": [
        "airbnb-base",
        "prettier",
        "plugin:node/recommended"
    ],
    "rules": {
        "prettier/prettier": "error",
        "no-unused-vars": "warn",
        "no-console": "off",
        "func-names": "off",
        "no-process-exit": "off",
        "object-shorthand": "off",
        "class-methods-use-this": "off"
    }
}